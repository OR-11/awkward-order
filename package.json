{
	"name": "soukousai25",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "pnpm run '/^build:.*/'",
		"build:app": "react-router build",
		"build:scripts": "pnpm --filter @latimeria/latimeria-cli run build",
		"build:shared": "pnpm --filter @latimeria/shared run build",
		"dev": "pnpm run '/^dev:.*/'",
		"dev:app": "react-router dev",
		"dev:assets-proxy": "pnpm --filter assets-proxy run dev",
		"dev:scripts": "pnpm --filter @latimeria/latimeria-cli run dev",
		"dev:shared": "pnpm --filter @latimeria/shared run dev",
		"start": "react-router-serve ./build/server/index.js",
		"lint": "biome check",
		"format": "biome format --write",
		"generate": "npm-run-all --parallel generate:*",
		"generate:cf-type": "wrangler types",
		"generate:react-router": "react-router typegen",
		"typecheck": "react-router typegen && tsc",
		"syncPublic": "latimeria gdrive",
		"db:generate": "drizzle-kit generate"
	},
	"dependencies": {
		"@hono/valibot-validator": "catalog:",
		"@latimeria/design-token": "^0.2.0",

		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"drizzle-kit": "^0.31.4",
		"npm-run-all2": "^8.0.4",
		"typescript": "catalog:",
		"unplugin-icons": "^22.2.0",
		"vite": "^7.1.3",
		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "catalog:"
	}

		"@latimeria/shared": "workspace:^",
		"@libsql/client": "^0.15.14",
		"@praha/drizzle-factory": "^1.2.1",
		"@react-router/node": "^7.8.1",
		"@react-router/serve": "^7.8.1",
		"@valibot/to-json-schema": "catalog:",
		"drizzle-orm": "^0.44.4",
		"drizzle-valibot": "^0.4.2",
		"hono": "catalog:",
		"hono-openapi": "catalog:",
		"isbot": "^5.1.30",
		"jotai": "^2.13.1",
		"react": "^19.1.1",
		"react-aria-components": "^1.11.0",
		"react-dom": "^19.1.1",
		"react-router": "^7.8.1",
		"uuid": "^11.1.0",
		"valibot": "catalog:"
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@cloudflare/vite-plugin": "^1.11.6",
		"@iconify-json/mdi": "^1.2.3",
